{
  "name": "ai-exam-readiness-checker",
  "version": "1.0.0",
  "description": "AI-powered exam readiness checker for high school students",
  "main": "server.js",
  "scripts": {
    "dev": "npm run cleanup && concurrently \"npm run server\" \"npm run client\"",
    "cleanup": "lsof -ti:5173 | xargs kill -9 2>/dev/null || true && lsof -ti:5000 | xargs kill -9 2>/dev/null || true",
    "server": "nodemon server/server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/server.js"
  },
  "keywords": [
    "ai",
    "education",
    "quiz",
    "hackathon"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf-text-extract": "^1.5.0",
    "pdf2json": "^3.1.6",
    "pdfjs-dist": "^5.3.93"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "pdf2pic": "^3.2.0"
  }
}
